<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>VSSDK.Shell.Interop.12</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Visual Studio "Shell" COM-interop assemblies used by Visual Studio 2013 and newer.

      Microsoft.VisualStudio.Shell.Interop.12.0.dll
    </description>
    <summary>Visual Studio (Shell Interop) for Visual Studio 2013+</summary>
    <tags>vssdk vs2013 vs2015</tags>
    <dependencies>
      <!-- Visual Studio 2013 -->
      <dependency id="VSSDK.IDE.12" version="[12.0.3,13.0.0)"/>
      <!--
      Exposed Dependencies (Microsoft.VisualStudio.Shell.Interop.12.0.dll):
        Microsoft.VisualStudio.OLE.Interop
        Microsoft.VisualStudio.Shell.Interop
        Microsoft.VisualStudio.Shell.Interop.10.0
        Microsoft.VisualStudio.Shell.Interop.11.0
        Microsoft.VisualStudio.Shell.Interop.8.0
      -->
      <dependency id="VSSDK.OLE.Interop" version="[7.0.3,8.0.0)"/>
      <dependency id="VSSDK.Shell.Interop" version="[7.0.3,8.0.0)"/>
      <dependency id="VSSDK.Shell.Interop.8" version="[8.0.3,9.0.0)"/>
      <dependency id="VSSDK.Shell.Interop.10" version="[10.0.3,11.0.0)"/>
      <dependency id="VSSDK.Shell.Interop.11" version="[11.0.3,12.0.0)"/>
      <!-- Also reference version from 2008 -->
      <dependency id="VSSDK.Shell.Interop.9" version="[9.0.3,10.0.0)"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v4.0\Microsoft.VisualStudio.Shell.Interop.12.0.dll" target="lib\net20"/>
    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v4.0\Microsoft.VisualStudio.Shell.Interop.12.0.xml" target="lib\net20"/>
  </files>
</package>
