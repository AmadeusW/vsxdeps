<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Tvl.VisualStudio.Dependencies.Language.CSharp.12</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Visual Studio "C# Language Service" reference assembly used by Visual Studio 2013.
    </description>
    <summary>Visual Studio (C# Language Service) for Visual Studio 2013</summary>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System.Core" targetFramework="net40"/>
      <frameworkAssembly assemblyName="PresentationCore" targetFramework="net40"/>
      <frameworkAssembly assemblyName="WindowsBase" targetFramework="net40"/>
    </frameworkAssemblies>
    <dependencies>
      <!-- Visual Studio 2013 -->
      <dependency id="Tvl.VisualStudio.Dependencies.12" version="[12.0.0,13.0.0)"/>
      <!--
      Exposed Dependencies (Microsoft.VisualStudio.CSharp.Services.Language.dll):
        EnvDTE
        EnvDTE80
        Microsoft.VisualStudio.CSharp.Services.Language.Interop
        Microsoft.VisualStudio.Language.CallHierarchy
        Microsoft.VisualStudio.OLE.Interop
        Microsoft.VisualStudio.Shell.12.0
        Microsoft.VisualStudio.Shell.Interop.11.0
        Microsoft.VisualStudio.Text.Data
        Microsoft.VisualStudio.TextManager.Interop
        Microsoft.VisualStudio.TextManager.Interop.8.0
        PresentationCore
        System
        System.Core
        WindowsBase

      Exposed Dependencies (Microsoft.VisualStudio.CSharp.Services.Language.Interop.dll):
        (none)
      -->
      <dependency id="Tvl.VisualStudio.Dependencies.DTE.10" version="[10.0.0,11.0.0)"/>
      <dependency id="Tvl.VisualStudio.Dependencies.OLE.Interop.10" version="[10.0.0,11.0.0)"/>
      <dependency id="Tvl.VisualStudio.Dependencies.TextManager.Interop.10" version="[10.0.0,11.0.0)"/>
      <dependency id="Tvl.VisualStudio.Dependencies.Shell.12" version="[12.0.0,13.0.0)"/>
      <dependency id="Tvl.VisualStudio.Dependencies.Shell.Interop.11" version="[11.0.0,12.0.0)"/>
      <dependency id="Tvl.VisualStudio.Dependencies.Text.12" version="[12.0.0,13.0.0)"/>
      <dependency id="Tvl.VisualStudio.Dependencies.Language.CallHierarchy.12" version="[12.0.0,13.0.0)"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="$VSIDE$\Microsoft.VisualStudio.CSharp.Services.Language.dll" target="lib\net45"/>

    <file src="$VSIDE$\Microsoft.VisualStudio.CSharp.Services.Language.Interop.dll" target="lib\net45"/>
  </files>
</package>
