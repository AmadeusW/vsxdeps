<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>VSSDK.LanguageService.10</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Visual Studio "MPF Language Service" reference assembly used by Visual Studio 2010 and newer.
    </description>
    <summary>Visual Studio (MPF Language Service) for Visual Studio 2010+</summary>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="Microsoft.MSXML" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System.Drawing" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System.Windows.Forms" targetFramework="net40"/>
      <frameworkAssembly assemblyName="System.Xml" targetFramework="net40"/>
    </frameworkAssemblies>
    <dependencies>
      <!-- Visual Studio 2010 -->
      <dependency id="VSSDK.IDE.10" version="[10.0.0,11.0.0)"/>
      <!--
      Exposed Dependencies (Microsoft.VisualStudio.Package.LanguageService.10.0.dll):
        Microsoft.MSXML
        Microsoft.VisualStudio.OLE.Interop
        Microsoft.VisualStudio.Shell.10.0
        Microsoft.VisualStudio.Shell.Interop
        Microsoft.VisualStudio.TextManager.Interop
        Microsoft.VisualStudio.TextManager.Interop.8.0
        System
        System.Drawing
        System.Windows.Forms
        System.Xml
      -->
      <dependency id="VSSDK.OLE.Interop.10" version="[10.0.0,11.0.0)"/>
      <dependency id="VSSDK.Shell.Interop.10" version="[10.0.0,11.0.0)"/>
      <dependency id="VSSDK.TextManager.Interop.10" version="[10.0.0,11.0.0)"/>
      <dependency id="VSSDK.Shell.10" version="[10.0.0,11.0.0)"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v4.0\Microsoft.VisualStudio.Package.LanguageService.10.0.dll" target="lib\net40"/>
    <file src="$VSSDK$\VisualStudioIntegration\Common\Assemblies\v4.0\Microsoft.VisualStudio.Package.LanguageService.10.0.xml" target="lib\net40"/>
  </files>
</package>
