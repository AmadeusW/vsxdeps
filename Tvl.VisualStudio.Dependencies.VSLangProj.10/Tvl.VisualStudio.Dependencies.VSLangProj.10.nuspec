<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Tvl.VisualStudio.Dependencies.VSLangProj.10</id>
    <version>0.0.0</version>
    <authors>Sam Harwell</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      This package provides the Automation and Extensibility (VSLangProj) COM-interop assemblies used by Visual Studio 2010 and newer.
    </description>
    <summary>Automation and Extensibility (VSLangProj) for Visual Studio 2010+</summary>
    <dependencies>
      <!-- Visual Studio 2010 -->
      <dependency id="Tvl.VisualStudio.Dependencies.10" version="[10.0.0,11.0.0)"/>
      <!-- EnvDTE -->
      <dependency id="Tvl.VisualStudio.Dependencies.DTE.10" version="[10.0.0,11.0.0)"/>
    </dependencies>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="install.ps1" target="tools"/>

    <!-- Reference assemblies -->

    <file src="$MSEnv$\PublicAssemblies\VSLangProj.dll" target="lib\net20"/>
    <file src="$MSEnv$\PublicAssemblies\VSLangProj.xml" target="lib\net20"/>

    <file src="$MSEnv$\PublicAssemblies\VSLangProj2.dll" target="lib\net20"/>
    <file src="$MSEnv$\PublicAssemblies\VSLangProj2.xml" target="lib\net20"/>

    <file src="$MSEnv$\PublicAssemblies\VSLangProj80.dll" target="lib\net20"/>
    <file src="$MSEnv$\PublicAssemblies\VSLangProj80.xml" target="lib\net20"/>

    <file src="$MSEnv$\PublicAssemblies\VSLangProj90.dll" target="lib\net20"/>
    <file src="$MSEnv$\PublicAssemblies\VSLangProj90.xml" target="lib\net20"/>

    <file src="$MSEnv$\PublicAssemblies\VSLangProj100.dll" target="lib\net20"/>
    <file src="$MSEnv$\PublicAssemblies\VSLangProj100.xml" target="lib\net20"/>
  </files>
</package>
